{
  "scripts": {
    "dev": "rimraf .next && cross-env NODE_ENV=development next -p 5047",
    "deploy": "npm run build && npm run export",
    "build": "rimraf .next && cross-env NODE_ENV=production next build",
    "export": "rimraf out && cross-env NODE_ENV=production next export && node scripts/nojekyll.mjs",
    "server": "serve out -p 5048",
    "eslint": "eslint ./ --fix",
    "update-version": "milktea-esm update",
    "prepare": "husky install"
  },
  "packageManager": "yarn@3.0.2",
  "engines": {
    "pm": "yarn@^3.0.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.17.0-alpha.8",
    "core-js": "^3.19.0",
    "highlight.js": "^11.3.1",
    "next": "^12.0.1",
    "pixi-live2d-display": "^0.3.1",
    "pixi.js": "^6.1.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-no-ssr": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@sweet-milktea/babel-preset-sweet": "^3.1.1",
    "@sweet-milktea/milktea-cli": "^3.1.1",
    "@sweet-milktea/util-tools": "^3.1.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^11.1.4",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "serve": "^12.0.1",
    "style-loader": "^3.3.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
