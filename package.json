{
  "name": "solar-system",
  "scripts": {
    "start": "rimraf .babel-build-cache && cross-env NODE_ENV=development gulp",
    "build:dev": "GOOS=js GOARCH=wasm go build -o example/solar-system.wasm",
    "build": "rimraf dist && GOOS=js GOARCH=wasm go build -o dist/solar-system.wasm",
    "build:tinygo": "rimraf dist && mkdir dist && tinygo build -o dist/solar-system.wasm -target=wasm",
    "prepare": "husky install"
  },
  "dependencies": {
    "three": "^0.127.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.13.14",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@sweet-milktea/babel-preset-sweet": "^2.2.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "fibers": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-better-rollup": "^4.0.1",
    "gulp-changed": "^4.0.2",
    "gulp-memory-fs": "^2.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-pug": "^4.0.1",
    "gulp-sass": "^4.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.10",
    "postcss-modules": "^4.0.0",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "sass": "^1.32.8"
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
