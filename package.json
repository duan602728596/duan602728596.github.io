{
  "scripts": {
    "dev": "rimraf .next && cross-env NODE_ENV=development next",
    "dev:preact": "rimraf .next && cross-env NODE_ENV=development PREACT=1 next",
    "build": "rimraf .next && cross-env NODE_ENV=production next build",
    "export": "rimraf out && cross-env NODE_ENV=production next export && node nojekyll.js",
    "server": "serve out",
    "eslint": "eslint ./ --fix",
    "update": "milktea update"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "^4.3.3",
    "highlight.js": "^10.0.3",
    "next": "^9.4.4",
    "preact": "^10.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-no-ssr": "^1.1.0"
  },
  "devDependencies": {
    "@sweet-milktea/milktea-cli": "^0.14.0",
    "@sweet-milktea/util-tools": "^0.15.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "less": "^3.11.3",
    "less-loader": "^6.1.0",
    "lint-staged": "^10.2.9",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
