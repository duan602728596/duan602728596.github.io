{
  "scripts": {
    "dev": "rimraf .next && cross-env NODE_ENV=development next",
    "build": "rimraf .next && cross-env NODE_ENV=production next build",
    "export": "rimraf out && cross-env NODE_ENV=production next export && node nojekyll.js",
    "server": "serve out",
    "eslint": "eslint ./ --fix",
    "updateVersion": "milktea update",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "antd": "^4.15.2",
    "highlight.js": "^10.7.2",
    "next": "^10.1.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-no-ssr": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.13.14",
    "@sweet-milktea/babel-preset-sweet": "^2.2.0",
    "@sweet-milktea/milktea-cli": "^2.3.1",
    "@sweet-milktea/util-tools": "^0.16.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "lint-staged": "^10.5.4",
    "node-sass": "^5.0.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
