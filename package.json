    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.2",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "serve": "^13.0.2",
