{
  "scripts": {
    "dev": "rimraf .next && cross-env NODE_ENV=development next",
    "dev:preact": "rimraf .next && cross-env NODE_ENV=development PREACT=1 next",
    "build": "rimraf .next && cross-env NODE_ENV=production next build",
    "export": "rimraf out && cross-env NODE_ENV=production next export && node nojekyll.js",
    "server": "serve out",
    "eslint": "eslint ./ --fix",
    "update": "milktea update"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.7.0",
    "highlight.js": "^10.2.1",
    "next": "^9.5.5",
    "preact": "^10.5.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.0-rc.3",
    "react-dom": "^17.0.0-rc.3",
    "react-no-ssr": "^1.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "@sweet-milktea/milktea-cli": "^1.0.0",
    "@sweet-milktea/util-tools": "^0.16.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-plugin-import": "^1.13.1",
    "commitizen": "^4.2.1",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.4",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "lint-staged": "^10.4.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
