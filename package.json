{
  "scripts": {
    "dev": "rimraf .next && cross-env NODE_ENV=development next",
    "dev:preact": "rimraf .next && cross-env NODE_ENV=development PREACT=1 next",
    "build": "rimraf .next && cross-env NODE_ENV=production next build",
    "export": "rimraf out && cross-env NODE_ENV=production next export && node nojekyll.js",
    "server": "serve out",
    "eslint": "eslint ./ --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "antd": "^4.2.0",
    "highlight.js": "^10.0.1",
    "next": "^9.3.6",
    "preact": "^10.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-no-ssr": "^1.1.0"
  },
  "devDependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.0.4",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^7.0.0-rc.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "lint-staged": "^10.1.7",
    "node-sass": "^4.14.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.0",
    "style-loader": "^1.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
