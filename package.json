    "core-js": "^3.19.2",
    "highlight.js": "^11.3.1",
    "next": "^12.0.7",
    "pixi-live2d-display": "^0.3.1",
    "pixi.js": "^6.2.0",
    "prop-types": "^15.7.2",
    "react": "rc",
    "react-dom": "rc",
    "react-no-ssr": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^12.0.4",
    "@sweet-milktea/babel-preset-sweet": "^3.1.8",
